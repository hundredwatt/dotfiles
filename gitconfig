[core]
  editor = vim
  whitespace = trailing-space,space-before-tab
	autocrlf = input

[apply]
  whitespace = fix

[color]
  ui = 1 ;doesnt work for some reason ...

  interactive = true
  status = true
  branch = true
  diff = true
[alias]
  cm = commit
  co = checkout
	cb = checkout -b
  a  = add
  b  = branch
  d  = diff
  dc = diff --cached

[user]
	email = hundredwatt@users.noreply.github.com
	name = Jason Nochlin
[push]
	default = current
[commit]
	template = /Users/jason/.gitmessage
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
